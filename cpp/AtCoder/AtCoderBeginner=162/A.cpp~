
#include <iostream>
#include <iostream>
#include <bits/stdc++.h>
#include <string>
#include <numeric>
#include <vector>
#include <cmath>
using namespace std;


int main(){
    int n; cin >> n;
    string bruh = "No";
    while(n >=0) {
        if(n%10 == 7) {
            bruh = "Yes";
            break;
        }
        if(n/10 == 0){
            break;
        }
        n/=10;
    }

    cout << bruh << endl;



}
